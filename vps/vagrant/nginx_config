server {
    listen 80;

    server_name localhost;

    access_log  /var/log/nginx/access.log;
    error_log  /var/log/nginx/error.log;

    location / {
        try_files $uri @nasanomics;
    }

    location @nasanomics {
        include uwsgi_params;
        uwsgi_pass unix:/var/www/run/nasanomics.sock;
   }
}
